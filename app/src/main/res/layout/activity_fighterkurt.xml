        app:layout_constraintEnd_toStartOf="@+id/txtEvasion"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtFighterDMG"
        app:layout_constraintTop_toBottomOf="@+id/txtFighterDMG" />

    <TextView
        android:id="@+id/txtEvasion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/txtFighterDMG"
        app:layout_constraintTop_toBottomOf="@+id/txtFighterDMG" />

    <TextView
        android:id="@+id/txtAccuracy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/txtLuck"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtLuck"
        app:layout_constraintTop_toBottomOf="@+id/txtLuck" />

